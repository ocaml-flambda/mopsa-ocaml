{
    "iter": [
        // C iterators
        "c.iterators.program",
        "c.iterators.interproc",
        "c.iterators.goto",
        "c.iterators.switch",

        // Universal iterators
        "universal.iterators.intraproc",
        "universal.iterators.loops",
        "universal.iterators.interproc.inlining",
        "universal.iterators.unittest",

        // C desugar
        "c.desugar.loops",
        "c.desugar.aggregate",
        "c.desugar.andor",
        "c.desugar.stmt_expr",

        // C libraries
        "c.libs.mopsa",
        "c.libs.c_stdlib",

        // Memory model
        {
            "stack": "universal.heap.recency",
            "over": {
                "iter": [
                    {
                        "stack": "c.memory.cells.expand",
                        "over" : {
                            "iter" : [
                                "c.memory.cells.cell_2_num",
                                "c.machine_integers",
                                "universal.numeric.values.intervals"
                            ]
                        }
                    },
                    "c.memory.cells.pointer"
                ]
            }
        }
    ]
}
