{
    "language": "c",
    "domain": {
	"iter": [
            // C iterators
            "c.iterators.program",
            "c.iterators.interproc",
            "c.iterators.goto",
            "c.iterators.switch",

            // Universal iterators
            "universal.iterators.intraproc",
            "universal.iterators.loops",
            "universal.iterators.interproc.inlining",
            "universal.iterators.unittest",

	    // Stubs iterator
	    "stubs.iterator",

            // C desugar
            "c.desugar.low_level",
            "c.desugar.loops",
            "c.desugar.record_copy",
            "c.desugar.stub_init",
            "c.desugar.cond_expr",

            // C libraries
            "c.libs.libc",
            "c.libs.libmopsa",

	    // C resources
	    "c.resources.file",
	    "c.resources.common",

            // Memory model
            {
		"stack": "universal.heap.recency",
		"over": {
                    "iter": [
			{
                            "stack": "c.memory.cells.expand",
                            "over" : {
				"stack" : "c.memory.cells.cell_2_scalar",
				"over": {
				    "iter": [
					"c.memory.cells.pointer",
					"c.machine_numbers",
					"universal.numeric.values.integer_float_mix"
				    ]
				}
                            }
			}
                    ]
		}
            }
	]
    }
}
